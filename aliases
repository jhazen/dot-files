dockerws() {
  docker run -it -v `pwd`:/ws ws
}
ctf() {
  docker run -it -v `pwd`:/data ctf
}
title() {
  echo -ne "\e]0;$1\a"
}
alias ll='ls -l'
alias la='ls -la'
alias lh='ls -lh'
alias ltr='ls -ltr'
alias psg='ps aux | grep'
pandoc_wiki() {
    cd ~ &> /dev/null
    pandoc -V geometry:margin=3cm -f markdown -t latex $1 -o ~/Documents/Report.pdf
    cd - &> /dev/null
}
