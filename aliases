title() {
  echo -ne "\e]0;$1\a"
}
alias ll='ls -l'
alias la='ls -la'
alias lh='ls -lh'
alias ltr='ls -ltr'
alias psg='ps aux | grep'
vimjavarun() {
   cd `dirname $1`
   java `echo $1 | sed 's/.java//g' | sed 's/.*\///g'`
   cd - > /dev/null
}
